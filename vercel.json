{
  "version": 2,
  "builds": [
    {
      "src": "instaforms/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "instaforms/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "instaforms.settings"
  }
}

